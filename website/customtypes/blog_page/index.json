{
  "id": "blog_page",
  "json": {
    "Main": {
      "uid": {
        "config": {
          "label": "UID",
          "placeholder": "The page name displayed in the URL"
        },
        "type": "UID"
      },
      "draft": {
        "type": "Boolean",
        "config": {
          "placeholder_false": "false",
          "placeholder_true": "true",
          "default_value": true,
          "label": "Draft"
        }
      },
      "title": {
        "type": "StructuredText",
        "config": {
          "multi": "heading1",
          "label": "Title"
        }
      },
      "subline": {
        "type": "StructuredText",
        "config": {
          "multi": "paragraph,strong,em",
          "label": "Introduction"
        }
      },
      "publish_date": {
        "type": "Date",
        "config": {
          "label": "Publish date"
        }
      },
      "authors": {
        "type": "Group",
        "config": {
          "fields": {
            "name": {
              "type": "Text",
              "config": {
                "label": "Name"
              }
            }
          },
          "label": "Authors"
        }
      },
      "image": {
        "type": "Image",
        "config": {
          "constraint": {},
          "thumbnails": [],
          "label": "Image"
        }
      },
      "text": {
        "type": "StructuredText",
        "config": {
          "multi": "paragraph,preformatted,heading2,heading3,heading4,strong,em,hyperlink,image,embed,list-item,o-list-item",
          "allowTargetBlank": true,
          "label": "Text (Deprecated - Use SliceZone instead)"
        }
      },
      "body": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {}
        }
      }
    },
    "Meta & SEO": {
      "meta_title": {
        "type": "Text",
        "config": {
          "label": "Meta title",
          "placeholder": "Uses blog title if not set"
        }
      },
      "meta_description": {
        "type": "Text",
        "config": {
          "label": "Meta description",
          "placeholder": "Uses blog introduction if not set"
        }
      },
      "meta_keywords": {
        "type": "Text",
        "config": {
          "label": "Meta keywords",
          "placeholder": "Uses default keywords if unset. Example: keyword1, keyword2, keyword3"
        }
      },
      "meta_image": {
        "type": "Image",
        "config": {
          "constraint": {},
          "thumbnails": [],
          "label": "Social Media Preview (uses blog image if unset)"
        }
      }
    }
  },
  "label": "Blog Article",
  "repeatable": true,
  "status": true,
  "format": "custom"
}
