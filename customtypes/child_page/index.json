            },
            "blogposts_grid": {
              "type": "SharedSlice"
            },
            "cards_carousel": {
              "type": "SharedSlice"
            },
            "buy_sell_wallet": {
              "type": "SharedSlice"
            },
            "consensus_map": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "Meta & SEO": {
      "meta_title": {
        "type": "Text",
        "config": {
          "label": "Meta title",
          "placeholder": "Used default title if unset"
        }
      },
      "meta_description": {
        "type": "Text",
        "config": {
          "label": "Meta description",
          "placeholder": "Used default description if unset"
        }
      },
      "meta_keywords": {
        "type": "Text",
        "config": {
          "label": "Meta keywords",
          "placeholder": "Used default keywords if unset. Example: keyword1, keyword2, keyword3"
        }
      },
      "meta_image": {
        "type": "Image",
        "config": {
          "constraint": {},
          "thumbnails": [],
