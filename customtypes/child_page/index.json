{
  "id": "child_page",
  "label": "Child Page",
  "repeatable": true,
  "status": true,
  "format": "custom",
  "json": {
    "Main": {
      "uid": {
        "config": {
          "label": "UID",
          "placeholder": "The page name displayed in the URL"
        },
        "type": "UID"
      },
      "draft": {
        "type": "Boolean",
        "config": {
          "label": "Draft",
          "placeholder_false": "false",
          "placeholder_true": "true",
          "default_value": false
        }
      },
      "parent": {
        "type": "Link",
        "config": {
          "select": "document",
          "customtypes": ["page"],
          "label": "Parent Page"
        }
      },
      "headerBackgroundColor": {
        "type": "Select",
        "config": {
          "options": ["white", "grey", "blue-dark", "blue", "green", "blue-s3"],
          "default_value": "white",
          "label": "Header background color"
        }
      },
      "footerBackgroundColor": {
        "type": "Select",
        "config": {
          "options": ["white", "grey", "blue-dark"],
          "default_value": "white",
          "label": "Footer background color"
        }
      },
      "show_table_of_contents": {
        "type": "Select",
        "config": {
          "options": ["None", "Short (Only h2)", "Long (h2 and h3)"],
          "default_value": "None",
          "label": "Show table of contents"
        }
      },
      "show_disclaimer": {
        "type": "Boolean",
        "config": {
          "placeholder_false": "No",
          "placeholder_true": "Yes",
          "default_value": false,
          "label": "Show Disclaimer"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "prestaking_grid": {
              "type": "SharedSlice"
            },
            "crypto_map_continent_selector": {
              "type": "SharedSlice"
            },
            "zig_zag_content": {
              "type": "SharedSlice"
            },
            "simple_headline": {
              "type": "SharedSlice"
            },
            "apps_showcase": {
              "type": "SharedSlice"
            },
            "pill_link": {
              "type": "SharedSlice"
            },
            "hero_section": {
              "type": "SharedSlice"
            },
            "logos_grid": {
              "type": "SharedSlice"
            },
            "albatross_liveview": {
              "type": "SharedSlice"
            },
            "grid_section": {
              "type": "SharedSlice"
            },
            "banner_slice": {
              "type": "SharedSlice"
            },
            "team_members": {
              "type": "SharedSlice"
            },
            "youtube_video": {
              "type": "SharedSlice"
            },
            "tilted_media": {
              "type": "SharedSlice"
            },
            "whitepaper_slice": {
              "type": "SharedSlice"
            },
            "social_media_grid": {
              "type": "SharedSlice"
            },
            "stepped_slides": {
              "type": "SharedSlice"
            },
            "staking_slice": {
              "type": "SharedSlice"
            },
            "stepped_lottie": {
              "type": "SharedSlice"
            },
            "rich_text_carousel": {
              "type": "SharedSlice"
            },
            "simple_carousel": {
              "type": "SharedSlice"
            },
            "new_york_grid": {
              "type": "SharedSlice"
            },
            "rich_text_cards": {
              "type": "SharedSlice"
            },
            "puzzle_grid": {
              "type": "SharedSlice"
            },
            "newsletter_subscription": {
              "type": "SharedSlice"
            },
            "rich_text": {
              "type": "SharedSlice"
            },
            "link_grid": {
              "type": "SharedSlice"
            },
            "lottie_slice": {
              "type": "SharedSlice"
